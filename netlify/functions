const { Client } = require('guilded.js');

exports.handler = async (event, context) => {
  const client = new Client({ token: process.env.GUILDED_TOKEN });

  await client.login();

  client.on('ready', () => {
    console.log("Bot is ready!");
  });

  return {
    statusCode: 200,
    body: JSON.stringify({ message: 'Bot started successfully' }),
  };
};
